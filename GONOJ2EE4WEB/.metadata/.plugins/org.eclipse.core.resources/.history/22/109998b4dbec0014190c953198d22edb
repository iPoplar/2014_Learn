package com.yang.entity;

import java.io.Serializable;
import java.util.Date;

public class Messageinfo implements Serializable {
	private int messageId;
	private String messageTitle;
	private String messageContent;
	private Date createTime;
	private String author;
	public int getMessageId() {
		return messageId;
	}
	public void setMessageId(int messageId) {
		this.messageId = messageId;
	}
	public String getMessageTitle() {
		return messageTitle;
	}
	public void setMessageTitle(String messageTitle) {
		this.messageTitle = messageTitle;
	}
	public String getMessageContent() {
		return messageContent;
	}
	public void setMessageContent(String messageContent) {
		this.messageContent = messageContent;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	@Override
	public String toString() {
		return "Messageinfo [author=" + author + ", createTime=" + createTime
				+ ", messageContent=" + messageContent + ", messageId="
				+ messageId + ", messageTitle=" + messageTitle
				+ ", getAuthor()=" + getAuthor() + ", getCreateTime()="
				+ getCreateTime() + ", getMessageContent()="
				+ getMessageContent() + ", getMessageId()=" + getMessageId()
				+ ", getMessageTitle()=" + getMessageTitle() + ", getClass()="
				+ getClass() + ", hashCode()=" + hashCode() + ", toString()="
				+ super.toString() + "]";
	}
	

}
